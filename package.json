{
  "name": "rollup-plugin-import-raw",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "assets can be imported as strings using the ?raw suffix",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/kimverchan/rollup-plugin-import-raw"
  },
  "author": "kimverchan",
  "homepage": "https://github.com/kimverchan/rollup-plugin-import-raw/#readme",
  "bugs": "https://github.com/kimverchan/rollup-plugin-import-raw/issues",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/es/index.js",
    "default": "./dist/cjs/index.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "rollup -c"
  },
  "files": [
    "dist",
    "!dist/**/*.map",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "rollup",
    "plugin",
    "raw",
    "import"
  ],
  "peerDependencies": {
    "rollup": "^3.0.0||^4.0.0"
  },
  "peerDependenciesMeta": {
    "rollup": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^14.18.63",
    "prettier": "3.6.2",
    "rollup": "^4.45.1",
    "rollup-plugin-dts": "^6.2.1",
    "tslib": "^2.8.1"
  },
  "types": "./dist/index.d.ts"
}
